<?xml version="1.0" encoding="UTF-8"?>

<atlassian-plugin key="${atlassian.plugin.key}" name="${project.name}" plugins-version="2">
  <plugin-info>
    <description>${project.description}</description>
    <version>${project.version}</version>
    <vendor name="${project.organization.name}" url="${project.organization.url}"/>
    <param name="plugin-icon">images/pluginIcon.png</param>
    <param name="plugin-logo">images/pluginLogo.png</param>
  </plugin-info>
  <!-- add our i18n resource -->
  <resource type="i18n" name="i18n" location="nowaZakladka"/>
  <!-- add our web resources -->
  <web-resource key="nowaZakladka-resources" name="nowaZakladka Web Resources">
    <dependency>com.atlassian.auiplugin:ajs</dependency>
    <resource type="download" name="nowaZakladka.css" location="/css/nowaZakladka.css"/>
    <resource type="download" name="nowaZakladka.js" location="/js/nowaZakladka.js"/>
    <resource type="download" name="images/" location="/images"/>
    <context>nowaZakladka</context>
  </web-resource>
  <issue-tabpanel key="nasza-nowa-zakladka" name="Nasza Nowa Zakladka" i18n-name-key="nasza-nowa-zakladka.name" class="pl.hycom.jira.plugins.gitlab.integration.nowaZakladka.NaszaNowaZakladka">
    <description key="nasza-nowa-zakladka.description">The Nasza Nowa Zakladka Plugin</description>
    <label key="nasza-nowa-zakladka.label"></label>
    <order>10</order>
    <resource type="velocity" name="view" location="templates/tabpanels/nasza-nowa-zakladka.vm"/>
  </issue-tabpanel>
</atlassian-plugin>
